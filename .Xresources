!XfSettings
Xft.dpi: 238
Xcursor*size: 48

URxvt.font:     xft:Anonymice Powerline:size=13:antialias=true
URxvt*faceName: Anonymice Powerline
URxvt.letterSpace: 2

!(URxvt terminal)
!URxvt.font:	xft:Anonymice Powerline:size=13
URxvt.transparent:	false
URxvt.scrollBar:        false
!Xft.antialias:		true
Xft.rgba:		rgba
Xft.hinting:		true
URxvt*termName: rxvt-unicode-256color

! Properties
! ! do not scroll with output
URxvt*scrollTtyOutput: false

URxvt*visualBell:  true
URxvt.cursorBlink: 1
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

URxvt.perl-ext-common:  selection-to-clipboard

!Disables ISOMODE 14755
URxvt.iso14755:         true
URxvt.iso14755_52:      true

!Rofi settings
rofi.dpi:   238
rofi.color-enabled: true
rofi.color-window: #30343f, #0effac, #a2b2d7
rofi.color-normal: argb:0000000, #ffffff, argb:0000000, #00b979, #eee8d5
rofi.color-active: #fdf6e3, #268bd2, #eee8d5, #268bd2, #fdf6e3
rofi.color-urgent: #fdf6e3, #dc322f, #eee8d5, #dc322f, #fdf6e3

URxvt*borderColor:        #151515
URxvt*colorBD:            #ffffff
URxvt*cursorColor:        #7FFF00
URxvt*cursorColor2:       #151515



*background: #171717
*foreground: #f8f8f8
*cursorColor: #f8f8f8

